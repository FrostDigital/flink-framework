{
  "name": "@flink/demo-app",
  "version": "0.2.0-beta.0",
  "description": "Demo app used as documentation and to run integration tests",
  "private": true,
  "scripts": {
    "build": "flink build",
    "dev": "nodemon --exec \"flink run\"",
    "test": "flink run --entry spec/support/runner.ts",
    "test:watch": "nodemon --exec \"flink run --entry spec/support/runner.ts\"",
    "start": "flink run",
    "clean": "flink clean && rimraf dist"
  },
  "dependencies": {
    "@flink-app/api-docs-plugin": "^0.2.0-beta.0",
    "@flink-app/firebase-messaging-plugin": "^0.2.0-beta.0",
    "@flink-app/flink": "^0.2.0-beta.0",
    "@flink-app/jwt-auth-plugin": "^0.2.0-beta.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@flink-app/test-utils": "^0.2.0-beta.0",
    "@types/jasmine": "^3.7.1",
    "@types/node": "^15.0.1",
    "@types/uuid": "^8.3.0",
    "got": "^9.6.0",
    "jasmine": "^3.7.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.3",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "4.2.3"
  }
}
