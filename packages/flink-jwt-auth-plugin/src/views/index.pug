html
  head
    title= title
  body
    h1= "API Documentation"

    ul
      each route in routes
        li
          h2 #{route.method.toUpperCase()} #{route.routeProps.path} #{route.routeProps.mockApi ? " (mocked)" : ""}
          h3 Request schema
          pre
            code #{route.reqSchema ? JSON.stringify(route.reqSchema, null, 2) : "n/a"}
          h3 Response schema
          pre
