{
  "name": "@flink-app/flink",
  "version": "0.2.0-beta.9",
  "description": "Typescript only framework for creating REST-like APIs on top of Express and mongodb",
  "types": "dist/src/index.d.ts",
  "main": "dist/src/index.js",
  "repository": "FrostDigital/flink",
  "scripts": {
    "test": "jasmine-ts --preserve-symlinks --config=./spec/support/jasmine.json",
    "test:watch": "nodemon --ext ts --exec 'jasmine-ts  --config=./spec/support/jasmine.json'",
    "start": "ts-node src/index.ts",
    "postinstall": "npm run build",
    "prepublish": "npm run build",
    "build": "tsc --project tsconfig.dist.json",
    "watch": "nodemon --exec 'tsc --project tsconfig.dist.json'",
    "clean": "rimraf dist"
  },
  "bin": {
    "flink": "./dist/bin/flink.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.12",
    "@types/mongodb": "3.6.12",
    "@types/uuid": "^8.3.0",
    "ajv": "^8.2.0",
    "ajv-formats": "^2.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "folder-hash": "^4.0.1",
    "fs-extra": "^10.0.0",
    "mkdirp": "^1.0.4",
    "mock-json-schema": "^1.0.8",
    "mongodb": "^3.6.6",
    "node-color-log": "^5.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "tiny-glob": "^0.2.9",
    "ts-json-schema-generator": "^0.94.1",
    "ts-morph": "^11.0.0",
    "typescript": "4.2.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/folder-hash": "^4.0.0",
    "@types/jasmine": "^3.7.1",
    "@types/json-schema": "^7.0.7",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^15.0.1",
    "jasmine": "^3.7.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1"
  },
  "gitHead": "3d0141c01022b554c90391f6911845c26081853e"
}
