{
    "name": "@flink-app/bankid-plugin",
    "version": "0.12.1-alpha.24",
    "description": "Flink plugin for Swedish BankID authentication and document signing",
    "scripts": {
        "test": "node --preserve-symlinks -r ts-node/register -- node_modules/jasmine/bin/jasmine --config=./spec/support/jasmine.json",
        "test:watch": "nodemon --ext ts --exec 'jasmine-ts  --config=./spec/support/jasmine.json'",
        "prepare": "tsc --project tsconfig.dist.json",
        "watch": "tsc-watch --project tsconfig.dist.json"
    },
    "author": "joel@frost.se",
    "license": "MIT",
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "bankid": "^3.2.1"
    },
    "overrides": {
        "@types/node": "22.13.10"
    },
    "devDependencies": {
        "@flink-app/flink": "^0.12.1-alpha.23",
        "@types/jasmine": "^3.7.1",
        "@types/node": "22.13.10",
        "jasmine": "^3.7.0",
        "jasmine-spec-reporter": "^7.0.0",
        "nodemon": "^2.0.7",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9",
        "typescript": "5.4.5"
    }
}
