{
  "name": "@flink-app/jwt-auth-plugin",
  "version": "0.2.0-alpha.13",
  "description": "Flink plugin that generates API documentation based on JSON schemas",
  "scripts": {
    "test": "node --preserve-symlinks -r ts-node/register -- node_modules/jasmine/bin/jasmine --config=./spec/support/jasmine.json",
    "test:watch": "nodemon --ext ts --exec 'jasmine-ts  --config=./spec/support/jasmine.json'",
    "prepublish": "tsc --project tsconfig.dist.json",
    "watch": "tsc-watch --project tsconfig.dist.json"
  },
  "author": "joel@frost.se",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "jwt-simple": "^0.5.6"
  },
  "devDependencies": {
    "@flink-app/flink": "^0.2.0-alpha.10",
    "@types/bcrypt": "^5.0.0",
    "@types/jasmine": "^3.7.1",
    "@types/node": "^15.6.2",
    "jasmine": "^3.7.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.4"
  },
  "gitHead": "3d0141c01022b554c90391f6911845c26081853e"
}
