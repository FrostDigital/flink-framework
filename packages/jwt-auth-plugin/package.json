        "jwt-simple": "^0.5.6"
    },
    "devDependencies": {
        "@flink-app/flink": "^0.12.1-alpha.7",
        "@types/bcrypt": "^5.0.0",
        "@types/jasmine": "^3.7.1",
        "@types/node": "22.13.10",
        "jasmine": "^3.7.0",
        "jasmine-spec-reporter": "^7.0.0",
        "nodemon": "^2.0.7",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9",
        "typescript": "5.4.5"
    },
    "gitHead": "770328c460ab4c755162df2294875780dd4d3803"
